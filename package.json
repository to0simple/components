    "autoprefixer": "^6.7.6",
    "axe-webdriverjs": "^1.1.1",
    "browser-sync": "^2.26.7",
    "chalk": "^2.4.2",
    "codelyzer": "^6.0.0-next.1",
    "conventional-changelog": "^3.0.5",
    "dgeni": "^0.4.11",
